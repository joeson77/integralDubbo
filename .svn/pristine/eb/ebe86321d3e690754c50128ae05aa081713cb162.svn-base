*{
    padding:0px;
    margin: 0px;
}
#container{
    width: 100%;
    height: 900px;
}
#header{
    width:100%;
    height: 50px;
    background: #00B38A;
}
#logo{
    width:20%;
    height: 50px;
    float: left;
    padding-left: 10px;
}
#logo_text{
    float: left;
    line-height: 50px;
    font-size: 20px;
    color: white;
}
#exit{
    width:10%;
    height: 50px;
    line-height: 50px;
    float: right;
    font-size: 14px;
}
#exit a{
    color: white;
    text-decoration: none;
}
#nav{
    width:10%;
    height: 800px;
    float: left;
    background: #1E1E1E;
}
#nav ul{
    width:100%;
    height:450px;
    padding-top: 30px;
    list-style:none;
    font-size: 14px;

}
#nav li{
    width:100%;
    height:30px;
    margin-left: 15px;
}
#nav a{
    text-decoration: none;
    color: #909090;
}
#nav a:hover{
    text-decoration: none;
    color: #00B38A;
}
#main{
    width:90%;
    height: 800px;
    float: left;
    background: #F8F8F8;
}
#content{
    width:80%;
    height: 700px;
    margin: 0 auto;
    margin-top: 20px;
    padding-top: 50px;
    background: white;
    box-shadow:1px 1px 5px #909090;
}
#achartsAjax{
    width:90%;
    margin: 0 auto;
    height: 500px;
}