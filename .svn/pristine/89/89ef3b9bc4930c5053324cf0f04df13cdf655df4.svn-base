<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.dao.AddRecordMapper">
  <insert id="addRecord" parameterType="com.test.entity.Integral" >
    insert into integral 
    (userName, updateTime, integral,ex1)
    values 
    (#{userName}, #{updateTime}, #{integral},#{ex1})
  </insert>
</mapper>